{
  "description": "Basic ebpfs programs used to track execve, open, openat, tcp_accept",
  "ebpf_config": {
    "folder": "ebpfs",
    "files": {
      "execve.c": {
        "types": ["kprobe", "kretprobe"],
        "description": "execve() system call",
        "header": "ebpf_config.h",
        "signature": ""
      },
      "_openat.c": {
        "types": ["kprobe"],
        "description": "open() & openat() system call",
        "header": "ebpf_config.h",
        "signature": ""
      },
      "_tcp_v4_accept.c": {
        "types": ["kretprobe"],
        "syscall": "inet_csk_accept",
        "description": "tcp v4 accept() system call",
        "header": "ebpf_config.h",
        "signature": ""
      }
    }
  }
}